.clearfix:after{
	content: "";
	display: block;
	height: 0;
	visibility: hidden;
	clear: both;
}
.fr{
	float: right;
}
.fl{
	float: left;
}
.spacing{
	padding: 10px 20px;
	font-size: 16px;
	color: #444;
}
