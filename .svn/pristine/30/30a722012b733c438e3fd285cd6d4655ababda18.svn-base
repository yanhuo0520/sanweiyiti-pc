<script>
export default {
  name: "organizationList",
  data() {
    return {
      tableData: [],
      curPage: 1,
      totalPage: null,
      loading: false,
      submitLoading: false,

      isTurn: false,
      ruleForm: {
        reason: ""
      },
      rules: {
        reason: [{ required: true, message: "请输入驳回原因", trigger: "blur" }]
      },
      formLabelWidth: "135px",
      form: {},
      address: "",
      isDetail: false,

      typeOptions: [{ name: "合作社", id: "0" }, { name: "合作联社", id: "1" }],
      statusOptions: [
        { name: "全部", id: "" },
        { name: "申请中", id: "0" },
        { name: "已通过", id: "1" },
        { name  : "已驳回", id: "2" },
        { name: "已注销", id: "3" }
      ],
      type: "0",
      cooperaName: "",
      shenpiStatus: "",
      curApproveStatus: '', // 当前数据的审批状态
      curRow: '', //当前展开的合作联社数据

      isAdmin: false,
      adminType: '',

      isApprove: false,
      isZhuxiao: false,
      isWxAuth: false,

      isSearch: false,

    };
  },
  activated() {
    let isRefresh = this.$route.query.isRefresh // 1不刷新 2或者undefined刷新
    if(isRefresh && isRefresh == 1) return
    this.type = "0";
    this.ruleForm.reason = "";
    //判断是否为管理员
    let adminType = localStorage.getItem("is_admin");
    if(adminType && Number(adminType) >= 1) {
        this.isAdmin = true
        this.adminType = adminType
        if(adminType != 3) {
          this.type = "1";
        }
    }
    
    this.tableData = [];
    this.curPage = 1;
    this.totalPage = null
    this.loading = true;
    this.cooperaName = '';
    this.shenpiStatus = '';
    this.getList();
    this.handlePermission();
  },
  methods: {
    initData() {
      this.type = "0";
      //判断是否为管理员
      let adminType = localStorage.getItem("is_admin");
      if(adminType && Number(adminType) >= 1) {
          this.isAdmin = true
          this.adminType = adminType
          if(adminType != 3) {
            this.type = "1";
          }
      }
      this.ruleForm.reason = "";
      this.tableData = [];
      this.totalPage = null;
      this.curPage = 1;
      this.cooperaName = '';
      this.shenpiStatus = '';
      this.loading = true;
      this.getList();
      this.handlePermission();
    },
    // 判断当前页面都有什么权限
    handlePermission() {
      let that = this;
      that.utils.getPermissionList(that, data => {
        data.forEach(item => {
          if (item.title == "审批") {
            that.isApprove = true;
          }
          if (item.title == "注销") {
            that.isZhuxiao = true;
          }
          if (item.title == "微信认证") {
            that.isWxAuth = true;
          }
        });
      });
    },
    // 切换合作联社还是合作社
    handleTabClick() {
      this.curPage = 1
      this.cooperaName = ''
      this.shenpiStatus = ''
      this.getList()
    },
    // 改变审批状态
    changeShenpiStatus() {
      this.curPage = 1
      this.isSearch = false
      this.getList()
    },
    // 清空合作社名称
    clearCooperName() {
      this.curPage = 1
      this.cooperaName = ''
      this.isSearch = false
      this.getList()
    },
    //列表
    getList() {
      this.loading = true
      this.ajax(
        "cooperaList",
        {
          page: this.curPage,
          size: 10,
          coopera_level: this.type,
          coopera_name: this.cooperaName,
          coopera_status: this.shenpiStatus
        },
        "查询失败",
        res => {
          this.loading = false;
          if (res.errno == 0) {
            this.tableData = res.data.data
            this.curPage = Number(res.data.current_page);
            this.totalPage = res.data.total;
          }
        },
        err => {
          this.loading = false;
        }
      );
    },
    search() {
      this.curPage = 1;
      this.totalPage = null;
      this.tableData = [];
      this.loading = true;
      this.isSearch = true;
      this.getList();
    },
    // 查看详情
    look(row) {
      this.form = row;
      this.address = row.coopera_address;
      this.isDetail = true;
      this.curApproveStatus = row.coopera_status

    },
    // 查看自己合作社
    lookChildList(row) {
      this.$router.push({
        path: 'childList',
        query: {
          name: row.coopera_name,
          id: row.coopera_id
        }
      })
    },
    //   注销
    writeOff(row) {
      this.form = row;
      this.$confirm("此操作将注销合作社, 是否继续?", "提示", {
        confirmButtonText: "确定",
        cancelButtonText: "取消",
        type: "warning"
      })
      .then(() => {
        this.checkCoopera("3");
      })
      .catch(() => {});
    },
    // 审核
    checkCoopera(status) {
      this.ajax(
        "updateCooperaStatus",
        {
          coopera_id: this.form.coopera_id,
          coopera_status: status,
          reject_note: this.ruleForm.reason
        },
        "审核失败",
        res => {
          if (res.errno == 0) {
            this.$message.success(res.errmsg);
            this.getList();
            if (status == 1) {
              this.isDetail = false;
            } else if (status == 2) {
              this.isTurn = false;
              this.isDetail = false;
            }
          }
        },
        err => {}
      );
    },
    // 驳回原因确认
    submitTurn(formName) {
      this.$prompt('请输入驳回原因', '提示', {
          confirmButtonText: '确定',
          cancelButtonText: '取消',
          inputPlaceholder: '请输入驳回原因 ',
          center: true
      }).then(({ value }) => {
        this.ruleForm.reason = value
        this.checkCoopera("2");
      }).catch(() => {});
      return
      this.$refs[formName].validate(valid => {
        if (valid) {
          this.checkCoopera("2");
        } else {
          setTimeout(() => {
            let isError = document.getElementsByClassName("is-error");
            let firstErrInput = isError[0].querySelector("input");
            if (firstErrInput.type == "file") {
              document.querySelectorAll(
                ".el-scrollbar__wrap"
              )[1].scrollTop = 50;
            } else {
              firstErrInput.focus();
            }
          }, 100);
          return false;
        }
      });
    },
    toWeixinAuth(id) {
      this.$router.push({
        path: "/weixinAuth",
        query: {
          coopera_id: id
        }
      });
    },
    // 切换分页
    handleCurPageChange(val) {
      this.curPage = val;
      this.getList();
    },
  }
};
</script>
